{
  "name": "dhedge",
  "license": "UNLICENSED",
  "scripts": {
    "codegen": "graph codegen",
    "codegen:dhedge": "npm run codegen -- subgraphs/dhedge.yaml",
    "codegen:rates": "npm run codegen -- subgraphs/rates.yaml",
    "build": "graph build",
    "deploy": "graph deploy --node https://api.thegraph.com/deploy/ --ipfs https://api.thegraph.com/ipfs/ sekmet/dhedge",
    "create-local": "graph create --node http://localhost:8020/ dhedge/dhedge",
    "remove-local": "graph remove --node http://localhost:8020/ dhedge/dhedge",
    "deploy-local": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 dhedge/dhedge",
    "build-all": "yarn run codegen dhedge.yaml && yarn run build dhedge.yaml && yarn run remove-local && yarn run create-local dhedge.yaml && yarn run deploy-local dhedge.yaml"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "0.18.0",
    "@graphprotocol/graph-ts": "0.18.0"
  }
}
